{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/react/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "storage.*"
  ],
  "modes": {
    "window": {
      "title": "app",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/react/public/logo512.png",
      "enableInspector": true,
      "tokenSecurity": "none"
    }
  },
  "cli": {
    "binaryName": "app",
    "resourcesPath": "/react/build/",
    "clientLibrary": "/react/public/neutralino.js",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/react/public/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}